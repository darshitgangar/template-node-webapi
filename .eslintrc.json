{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "comma-dangle": ["error", "only-multiline"],

        "no-console": "off"
    }
}